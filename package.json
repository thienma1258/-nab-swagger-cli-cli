{
  "name": "@thienma1258/nab-swagger-cli",
  "description": "Build swagger-compliant REST APIs using TypeScript and Node",
  "version": "1.0.2",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "typescript",
    "openapi",
    "swagger",
    "server",
    "node",
    "node.js",
    "codegen",
    "generation",
    "express"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc && yarn copy-templates",
    "copy-templates": "copyfiles -u 1 ./src/routeGeneration/templates/**/* ./dist",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc": "tsc -b",
    "watch": "tsc -b -w"
  },
  "author": "Luke Autry <lukeautry@gmail.com> (http://www.lukeautry.com) modify by dongpham",
  "license": "MIT",
  "dependencies": {
    "@microsoft/tsdoc-config": "^0.15.2",
    "@tsoa/runtime": "^3.6.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "merge": "^2.1.0",
    "minimatch": "^3.0.4",
    "moment": "^2.24.0",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.1.2",
    "validator": "^12.2.0",
    "yamljs": "^0.3.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/minimatch": "^3.0.3",
    "@types/node": "^14.11.1",
    "@types/yamljs": "^0.2.30",
    "@types/yargs": "^13.0.2",
    "copyfiles": "^2.2.0",
    "rimraf": "^3.0.0"
  },
  "bin": {
    "nab-swagger-cli": "dist/cli.js"
  },
  "engines": {
    "yarn": ">=1.9.4",
    "node": ">=6.0.0"
  },
  "repository": {
    "url": "git@github.com:thienma1258/nab-swagger-cli.git",
    "type": "git"
  },
  "engineStrict": true,
  "publishConfig": {
    "registry":"https://npm.pkg.github.com/"
  }
}
